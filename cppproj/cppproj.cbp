<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="cppproj" />
		<Option makefile_is_custom="1" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="debug/dynamic/cppproj" prefix_auto="1" extension_auto="1" />
				<Option object_output="debug/dynamic/obj/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add directory="cppl/cppproj/src/include" />
				</Compiler>
				<MakeCommands>
					<Build command="$make -f $makefile debugdynamic" />
					<CompileFile command="$make -f $makefile $file" />
					<Clean command="$make -f $makefile  clean pch buildlibslinux buildarchivelinux " />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="$make -q -f $makefile debugdynamic" />
					<SilentBuild command="$make -f $makefile debugdynamic &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
			<Target title="Release">
				<Option output="release/dynamic/cppproj" prefix_auto="1" extension_auto="1" />
				<Option object_output="release/dynamic/obj/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add directory="cppl/cppproj/src/include" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
				<MakeCommands>
					<Build command="$make -f $makefile releasedynamic" />
					<CompileFile command="$make -f $makefile $file" />
					<Clean command="$make -f $makefile clean pch buildlibslinux buildarchivelinux " />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="$make -f $makefile -q releasedynamic" />
					<SilentBuild command="$make -f $makefile releasedynamic &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
		</Compiler>
		<ExtraCommands>
			<Add before="./prebuild" />
		</ExtraCommands>
		<Unit filename="Makefile" />
		<Unit filename="src/include/ArgParser.cpp" />
		<Unit filename="src/include/ArgParser.h" />
		<Unit filename="src/include/Directories.cpp" />
		<Unit filename="src/include/Directories.h" />
		<Unit filename="src/include/Files.cpp" />
		<Unit filename="src/include/Files.h" />
		<Unit filename="src/include/SignalHandler.cpp" />
		<Unit filename="src/include/SignalHandler.h" />
		<Unit filename="src/include/cppproj.cpp" />
		<Unit filename="src/include/cppproj.h" />
		<Unit filename="src/main.cpp" />
		<Extensions />
	</Project>
</CodeBlocks_project_file>
